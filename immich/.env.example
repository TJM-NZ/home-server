# Copy this file to .env and fill in your values
# Full docs: https://docs.immich.app/install/environment-variables

# Path where uploaded photos are stored
UPLOAD_LOCATION=/mnt/immich-storage

# Path where the Postgres database files are stored
DB_DATA_LOCATION=./postgres

# Timezone (see https://en.wikipedia.org/wiki/List_of_tz_database_time_zones)
TZ=Pacific/Auckland

# Immich version to use (pin to a specific version like "v2.1.0", or use "v2" for latest v2.x)
IMMICH_VERSION=v2

# Postgres database password â€” generate a random string, alphanumeric only
DB_PASSWORD=your_password_here

# These typically don't need changing
DB_USERNAME=postgres
DB_DATABASE_NAME=immich
